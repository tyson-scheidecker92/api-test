{
  "scripts": {
    "start": "node server.js",
    "test": "jest --coverage"
  },
  "type": "module",
  "dependencies": {
    "express": "^4.21.1",
    "moment-timezone": "^0.5.46"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "*.{js,ts}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
